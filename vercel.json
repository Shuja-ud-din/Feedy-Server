{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/users/(.*)",
            "methods": [
                "POST",
                "GET"
            ],
            "dest": "/server.js"
        }
    ]
}